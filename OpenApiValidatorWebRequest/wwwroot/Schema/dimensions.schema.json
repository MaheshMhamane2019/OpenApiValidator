{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "dimensions",
	"title": "Dimensions",
	"type": "object",
	"properties": {
		"length": {
			"type": "number",
			"exclusiveMinimum": 0
		},
		"width": {
			"type": "number",
			"exclusiveMinimum": 0
		},
		"height": {
			"type": "number",
			"exclusiveMinimum": 0
		}
	},
	"required": [ "length", "width", "height" ]
}
